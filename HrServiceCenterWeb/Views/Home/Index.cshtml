@{
    ViewBag.Title = "Home Page";
}
<script src="~/Library/echarts/js/echarts.min.js"></script>
<script src="~/Library/echarts/theme/infographic.js"></script>
<script src="~/JS/chart.HomePage.js"></script>
<script src="~/JS/vue.min.js"></script>
<div class="container" style="padding-top:10px;">
    <div id="employeeCounter" style="width: 100%;height:300px;display:none;" class="row"></div>
    <div class="row" style="background-color:rgba(199, 225, 243, 0.30)">
        <div class="col-md-4">
            <div id="employeePositionCounter" style="width: 100%;height:300px;"></div>
        </div>
        <div class="col-md-2"></div>
        <div class="col-md-4">
            <div id="employeeDegreeCounter" style="width: 100%;height:300px;"></div>
        </div>
    </div>
    <div style="height:10px;"></div>
    <div class="row" style="background-color:rgba(199, 225, 243, 0.30);">
        <div id="payChart" style="height:300px;"></div>
    </div>
    <div style="height:10px;"></div>
    <div class="row">
        <div id="app" class="home_panel">
            <div class="home_panel_title">合同即将到期人员</div>
            <ol>
                <li v-for="p in persons">
                    <span>{{ p.PersonName }} </span> /
                    <span> {{ p.ContractTime }} / </span>
                    <span> {{ p.ContractDays }}天 </span>/
                    <span> {{ p.CompanyName }} </span>/
                    <span> {{ p.CardId }} </span>/
                    <span> {{ p.PositionName }} </span>/
                    <span> {{ p.Phone }} </span>
                </li>
            </ol>
        </div>
    </div>
    <div style="height:10px;"></div>
    <div class="row">
        <div id="app2" class="home_panel">
            <div class="home_panel_title">即将退休人员</div>
            <ol>
                <li v-for="p in persons">
                    <span>{{ p.PersonName }} </span> /
                    <span> {{ p.RetireTime }} / </span>
                    <span> {{ p.RetireDays }}天 </span>/
                    <span> {{ p.CompanyName }} </span>/
                    <span> {{ p.CardId }} </span>/
                    <span> {{ p.PositionName }} </span>/
                    <span> {{ p.Phone }} </span>
                </li>
            </ol>
        </div>
    </div>
</div>
<script>

</script>