
@{
    ViewBag.Title = "缴存发放";
}
<!-- import opt js-->
<script src="~/JS/pay.PaySendOpt.js"></script>
<script type="text/javascript">
    var dataId = 0;
</script>

<div class="container container-layout">
    <div class="page-header">
        <h3>发放管理.<small>&nbsp;<a href="~/Pay/PayList">发放列表</a></small>.<small>&nbsp;发放表</small></h3>
    </div>
    <div style="margin:20px 0;"></div>
    <div>
        <form class="form-horizontal" id="formCompany">

            <div class="form-group">
                <label class="col-sm-1 control-label">发放月份</label>
                <div class="col-sm-10">
                    <input id="datebox" editable="false" name="attYearMonth" class="easyui-datebox" style="height:30px;width:150px" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-1 control-label">公司名称</label>
                <div class="col-sm-10">
                    <input id="cmpname" class="easyui-combobox" form-field-name="CompanyName" form-control-type="textbox" data-options="valueField: 'CompanyId',textField: 'Name',required:true,editable:false" style="height:30px;width:50%" />
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-1 control-label">发放名称</label>
                <div class="col-sm-10">
                    <input type="text" id="tempname" style="height:30px;width:50%" class="form-control panelseach-text" data-options="required:true">
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-10">
                    <button id="creatbtn" class="btn btn-primary" onclick="opt.loadTable();return false;">创建发放表</button>
                    <button id="savebtn" class="btn btn-success" onclick="return false;">保存发放数据</button>
                    <button id="btnExport" class="btn btn-success" onclick="return false;">导出数据</button>
                    <button id="btnImport" class="btn btn-success" onclick="return false;">导入数据</button>
                    <button id="filebtn" class="btn btn-warning" onclick="opt.onfile();return false;">提交归档</button>
                </div>
            </div>
        </form>
    </div>


    <div id="dgContainer" style="margin:20px 0;">
        <table id="dg" class="easyui-datagrid" title="" style="width:100%;height:100%"
               data-options="idField:'a',
			        singleSelect:true,collapsible:true,rownumbers:true,striped:true,
			        autoRowHeight:true,
                    autoEditing: true,
                    fit: true"></table>
    </div>
    <input id="hideview" type="hidden" value="@ViewBag.PayId" />
</div>

